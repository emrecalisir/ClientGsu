<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.clientgsu.activity.MainActivity" >

    <EditText
        android:id="@+id/editText1"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:ems="10"
        android:text="192.168.1.2" >

        <requestFocus />
    </EditText>

    <Button
        android:id="@+id/buttonBrowse"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="35dp"
        android:text="@string/lblButtonBrowse" />

    <Button
        android:id="@+id/buttonSend"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="90dp"
        android:text="@string/lblButtonSend" />

    <Button
        android:id="@+id/buttonDoLocally"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="90dp"
        android:text="@string/lblButtonDoLocally" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_alignLeft="@+id/buttonSend"
        android:layout_marginTop="150dp"
        android:scaleType="matrix" />

    <TextView
        android:id="@+id/TextViewServer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="10dp"
        android:text="SERVER" />
    
    <TextView
        android:id="@+id/TextViewClient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="10dp"
        android:text="CLIENT" />
        
    <TextView
        android:id="@+id/TextViewA1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="30dp"
        android:text="a1" />

        <TextView
        android:id="@+id/TextViewB1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="50dp"
        android:text="b1" />
    
        
        <TextView
        android:id="@+id/TextViewD1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="30dp"
        android:text="d1" />
    
    <TextView
        android:id="@+id/TextViewC1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="70dp"
        android:text="c1" />

    <TextView
        android:id="@+id/TextViewTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="90dp"
        android:text="total" />

</RelativeLayout>